# Local directory automatic Qt GUI

#setup

rpm -i test-patches/i386/update/9.1/rpm/i586/mydummy-1.0-0.i586.rpm
Xvfb :89 &

#test

export DISPLAY=:89

/usr/lib/YaST2/bin/y2base online_update '(.nosigcheck)' '(.url)' \
  '("file:/usr/src/packages/BUILD/yast2-online-update-test/test-patches/")' qt \
  --macro tests/you1.macro.ycp

[ "`rpm -q mydummy`" = "mydummy-1.0-1" ]
[ "`rpm -q mydummy2`" = "mydummy2-1.0-0" ]


#cleanup

kill `cat /tmp/.X89-lock`
